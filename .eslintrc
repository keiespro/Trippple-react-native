{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "globals": {
      "__DEV__": true,
      "__DEBUG__": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "extends": [
        # "eslint:recommended",
        "airbnb",
        "plugin:react/recommended"
    ],
    "rules": {
      "comma-dangle": 0,
      "radix": [1, "as-needed"],
      "global-require": 0,
      "semi": 0,
      "react/prop-types": 0,
      "one-var": 0,
      "no-use-before-define": 0,
      "import/extensions": [1, "never"],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-bind": 0,
      "eqeqeq": 0,
      "max-len": 0,
      "no-unused-expressions": 0,
      "object-curly-spacing": 0,
      "no-underscore-dangle": 0,
      "space-before-blocks": 0,
      "react/jsx-space-before-closing": 0,
      "arrow-parens": 0
    }
}
