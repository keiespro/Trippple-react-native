{
    "parser": "babel-eslint",


    "ecmaFeatures": {
      "jsx": true
    },

    "settings": {
        "ecmascript": 7,
        "jsx": true
    },
    "plugins": [
      "react",
      "react-native"
    ],
    "rules": {
        "strict": 1,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "indent": [1,2],
        "react/jsx-closing-bracket-location": 0,
        "react/jsx-indent-props": [0,2],
        "react/jsx-indent": [0, 2],
        "react-native/no-unused-styles": 0

    }
}
