{
  "name": "Trippple",
  "version": "0.0.1",
  "private": true,
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "verbose": true,
    "collectCoverage": true,
    "setupEnvScriptFile": "node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-native",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys",
      "object.values",
      "es-abstract",
      "promise",
      "source-map"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development node node_modules/react-native/local-cli/cli.js start --reset-cache --verbose",
    "autoupdate": "NODE_ENV=production ./AutoUpdater/bin/deploy-ota.js",
    "dist": "NODE_ENV=production node node_modules/react-native/local-cli/cli.js bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/Trippple/main.jsbundle --assets-dest ./assets  --verbose",
    "sim-dist": "npm run dist; xcodebuild -project ios/Trippple.xcodeproj -scheme Trippple -destination id=00A44CEC-3D20-4E71-8123-C1C659CDD0B0 -derivedDataPath build",
    "sim": "NODE_ENV=development xcodebuild -project ios/Trippple.xcodeproj -scheme Trippple -destination id=00A44CEC-3D20-4E71-8123-C1C659CDD0B0 -derivedDataPath build",
    "unit-tests": "node_modules/mocha/bin/mocha --compilers js:./test/unit/compile.js ./test/unit/initTest.js",
    "unit-tests-watch": "node_modules/mocha/bin/mocha --compilers js:./test/unit/compile.js ./test/unit/initTest.js --watch",
    "integration-tests": "node_modules/mocha/bin/mocha --compilers js:babel-register ./test/integration/welcome.spec.js",
    "jest": "echo 'not used'; jest",
    "jestwatch": "echo 'not used'; jest --watch"
  },
