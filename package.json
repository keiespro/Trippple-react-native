{
  "name": "Trippple",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node node_modules/react-native/local-cli/cli.js start",
    "startclear": "NODE_ENV=development node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "autoupdate": "NODE_ENV=production ./AutoUpdater/bin/deploy-ota.js",
    "dist": "NODE_ENV=production node node_modules/react-native/local-cli/cli.js bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/Trippple/main.jsbundle --assets-dest ./assets  --verbose",
    "sim-dist": "npm run dist; xcodebuild -project ios/Trippple.xcodeproj -scheme Trippple -derivedDataPath build",
    "sim": "NODE_ENV=development xcodebuild -project ios/Trippple.xcodeproj -scheme Trippple -derivedDataPath build",
    "unit-tests": "node_modules/mocha/bin/mocha --compilers js:./test/unit/compile.js ./test/unit/initTest.js",
    "unit-tests-watch": "node_modules/mocha/bin/mocha --compilers js:./test/unit/compile.js ./test/unit/initTest.js --watch",
    "integration-tests": "node_modules/mocha/bin/mocha  ./test/helpers/driver.js",
    "test": "mocha ./test",
    "appium": "appium -p 4723",
    "dev": "./dev.sh"
  },
