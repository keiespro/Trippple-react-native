{
  "name": "Trippple",
  "version": "2.5.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle": "NODE_ENV=production react-native bundle  --entry-file index.ios.js --verbose --reset-cache  --platform ios —-dev false --assets-dest assets  --bundle-output main.jsbundle",
    "bundle-staging": "TRIPPPLE_SERVER=staging NODE_ENV=production react-native bundle  --entry-file index.ios.js --verbose --reset-cache  --platform ios —-dev false --assets-dest assets  --bundle-output main.jsbundle",
    "bundle-prod": "TRIPPPLE_SERVER=production NODE_ENV=production react-native bundle  --entry-file index.ios.js --verbose --reset-cache  --platform ios —-dev false --assets-dest assets  --bundle-output main.jsbundle",
    "unit-tests": "node_modules/mocha/bin/mocha --compilers js:./test/unit/compile.js ./test/unit/initTest.js",
    "unit-tests-watch": "node_modules/mocha/bin/mocha --compilers js:./test/unit/compile.js ./test/unit/initTest.js --watch",
    "integration-tests": "node_modules/mocha/bin/mocha  ./test/helpers/driver.js",
    "test": "mocha ./test",
    "appium": "appium -p 4723",
    "dev": "./dev.sh",
    "postinstall": "rndebugger-open",
    "testflight-staging": "npm run bundle-staging && cd ios && fastlane beta && cd -",
    "submit": "npm run bundle-prod && cd ios && fastlane appstore && cd -",
    "bundle-android-release": "TRIPPPLE_SERVER=production NODE_ENV=production react-native bundle  --entry-file index.android.js --verbose --reset-cache  --platform android —-dev false --bundle-output main.android.jsbundle"
  },
